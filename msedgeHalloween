local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "[üéÉHALLOWEENüéÉ] msedge - Slap Battlesüëè", HidePremium = false, SaveConfig = true, ConfigFolder = "msedge"})

-- values
_G.AutoSlapAll = true

-- tab
local MainTab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://16669860992",
	PremiumOnly = false
})

-- buttons
MainTab:AddButton({
	Name = "Candy Farm",
	Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/coolperson-sudo/SlapBattles/refs/heads/main/CandyFarm"))()
  	end    
})

MainTab:AddButton({
	Name = "Rejoin",
	Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Jelly-plays/Rejoin-script/main/obf_L744By559M18BbuseSG6en8r1zL31daK9060LV7WyvmS4bQp92aONWfwRE36FdcZ.lua.txt"))();
  	end    
})

MainTab:AddButton({
	Name = "Destroy GUI",
	Callback = function()
        OrionLib:Destroy()
  	end    
})

-- tab

local PoltergeistTab = Window:MakeTab({
	Name = "Poltergeist",
	Icon = "rbxassetid://16669860992",
	PremiumOnly = false
})

PoltergeistTab:AddButton({
	Name = "Join Poltergeist Place",
	Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/coolperson-sudo/SlapBattles/refs/heads/main/PoltergeistPlace"))()
  	end    
})

PoltergeistTab:AddToggle({
	Name = "Auto Slap Boss/Zombies",
	Default = false,
	Callback = function(Value)
		_G.AutoSlapAll = Value
while _G.AutoSlapAll do
for i,v in pairs(workspace.Enemies:GetChildren()) do
if v:FindFirstChild("HumanoidRootPart") then
game:GetService("ReplicatedStorage").Remotes.GeneralHit:FireServer(v:FindFirstChild("HumanoidRootPart"))
end
end
task.wait(0.15)
end
	end    
})

PoltergeistTab:AddButton({
	Name = "No Cooldown",
	Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/coolperson-sudo/SlapBattles/refs/heads/main/NoCooldown"))()
  	end    
})
